


# --- paths -------------------------------------------------------------

top_builddir=	@top_builddir@
top_srcdir=	@top_srcdir@
srcdir=		@srcdir@
builddir=       @builddir@

INSTALL=  	@INSTALL@

include $(top_builddir)/config.mk

# Ensure that the default target is the one defined in codels.mk
all: codels-mk-all

# --- source files ------------------------------------------------------

codels_src = \
	pr2SoftMotionMainCodels.cc \
	pr2SoftMotionCntrlTaskCodels.cc \
        ControllerAmbassador.cc \
        GripperSensorMonitor.cc

# Add extra flags for the preprocessor (external libraries, ...)
EXTRA_INCLUDES += \
  -I/opt/ros/cturtle/ros/core/roscpp/include \
  -I/opt/ros/cturtle/ros/core/roslib/include \
  -I/opt/ros/cturtle/ros/core/rosconsole/include \
  -I/opt/ros/cturtle/ros/3rdparty/xmlrpcpp/src \
  -I/opt/ros/cturtle/ros/core/roslib/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/common_msgs/geometry_msgs/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/pr2_controllers/pr2_controllers_msgs/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/common_msgs/sensor_msgs/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/common_msgs/actionlib_msgs/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/common/actionlib/include \
  -I/opt/ros/cturtle/stacks/common/actionlib/msg/cpp \
  -I/opt/ros/cturtle/stacks/common_msgs/actionlib_msgs/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/common_msgs/nav_msgs/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/pr2_controllers/trajectory_msgs/msg/cpp \
  -I/opt/ros/cturtle/stacks/pr2_controllers/trajectory_msgs/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/pr2_controllers/pr2_controllers_msgs/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/pr2_controllers/pr2_controllers_msgs/srv_gen/cpp/include \
  -I/u/gmanfred/ros_overlay/sandbox/pr2_soft_controller/include \
  -I/u/gmanfred/ros_overlay/sandbox/pr2_soft_controller/msg_gen/cpp/include \
  -I/opt/ros/cturtle/ros/std_msgs/include \
  -I/opt/ros/cturtle/ros/std_msgs/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/geometry/tf/include \
  -I/opt/ros/cturtle/stacks/geometry/tf/msg/cpp \
  -I/opt/ros/cturtle/stacks/geometry/tf/srv/cpp \
  -I/opt/ros/cturtle/stacks/geometry/tf/msg_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/geometry/tf/srv_gen/cpp/include \
  -I/opt/ros/cturtle/stacks/geometry/bullet/include \
  -I/opt/ros/cturtle/stacks/pr2_object_manipulation/pr2_gripper_sensor_msgs/msg_gen/cpp/include

#
# Add extra link flags (external libraries, ...)
EXTRA_LIBS += \
	-Wl,-rpath /opt/ros/cturtle/ros/core/roscpp/lib \
	-Wl,-rpath /opt/ros/cturtle/ros/core/rosconsole/lib \
	-Wl,-rpath /opt/ros/cturtle/ros/core/roslib/lib \
	-Wl,-rpath,/opt/ros/cturtle/stacks/common/actionlib/lib \
        -Wl,-rpath,/opt/ros/cturtle/stacks/geometry/tf/lib \
	-Wl,-rpath,/opt/ros/cturtle/stacks/geometry/bullet/lib \
        -Wl,-rpath,/opt/ros/cturtle/stacks/pr2_controllers/pr2_mechanism_controllers/lib \
        -Wl,-rpath,/opt/ros/cturtle/stacks/pr2_controllers/robot_mechanism_controllers/lib \
	-L/opt/ros/cturtle/ros/core/roscpp/lib -lros \
	-L/opt/ros/cturtle/stacks/geometry/tf/lib -ltf \
   	-L/opt/ros/cturtle/stacks/geometry/bullet/lib -lLinearMath \
   	-L/opt/ros/cturtle/stacks/common/actionlib/lib -lactionlib \
        
  


#
# Install files here (any header that is not included in the .gen for instance)
# Files that are included in the GenoM file are automatically installed
# use $(includedir) to install headers
# user-install: 

# --- compilation -------------------------------------------------------
include $(top_srcdir)/$(autoconfdir)/codels.mk
include $(top_srcdir)/$(autoconfdir)/config.$(OSAPI).mk


